Shared:
./configure -prefix $PWD -opensource -confirm-license -nomake tests -nomake examples -nomake demos -no-qt3support -release

-developer-build

Qt STATIC BUILD (tested with Qt4 on all platforms):
./configure -prefix $PWD -nomake demos -nomake examples -nomake tests -release -static -no-exceptions -qt-zlib -qt-libpng -qt-libjpeg -dont-process -no-qt3support [-arch x86] [-no-webkit]

bin/qmake projects.pro QT_BUILD_PARTS="libs" JAVASCRIPTCORE_JIT="yes"
make -jN